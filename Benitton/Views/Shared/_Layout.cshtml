        function checkWhitespace(str) {
            var spaceCount = 0;

            for (var i = 0; i < str.length; i++) {
                if (str[i] === ' ') {
                    spaceCount++;
                }
            }
            return spaceCount;
        }


        function validateInput(input) {
            var countryCode = $("#countryCode").val();
            var inputData = input.value;
            if (/[^0-9 ]/.test(inputData)) {
                document.querySelector('.MobileVal').style.display = 'block';
                return false;
            }
            if (countryCode == "+91 ") {
                input.maxLength = 10 + checkWhitespace(inputData);
                if (inputData.length < input.maxLength) {
                    input.setCustomValidity('Invalid mobile number.');
                    document.querySelector('.MobileVal').style.display = 'block';
                    return false;
                } else {
                    input.value = input.value.slice(0, input.maxLength);
                    input.setCustomValidity('');
                    document.querySelector('.MobileVal').style.display = 'none';
                    //input.preventDefault();
                    return false;
                }

            } else {
                input.maxLength = 15;
                if (inputData.length < 7) {
                    input.setCustomValidity('Invalid mobile number.');
                    document.querySelector('.MobileVal').style.display = 'block';
                    return false;
                } else if (inputData.length > 7 && inputData.length < 16) {
                    input.setCustomValidity('');
                    document.querySelector('.MobileVal').style.display = 'none';
                    return false;
                } else {
                    input.value = inputData.slice(0, input.maxLength);
                    input.setCustomValidity('');
                    document.querySelector('.MobileVal').style.display = 'none';
                    return false;
                }
            }

        }


