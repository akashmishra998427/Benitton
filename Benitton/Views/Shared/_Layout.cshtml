    <script type="text/javascript">
        function validateNumber(evt) {
            var theEvent = evt || window.event;
            if (theEvent.type === 'paste') {
                key = event.clipboardData.getData('text/plain');
            } else {
                // Handle key press
                var key = theEvent.keyCode || theEvent.which;
                key = String.fromCharCode(key);
            }
            var regex = /[0-9]|\./;
            if (!regex.test(key)) {
                theEvent.returnValue = false;
                if (theEvent.preventDefault) theEvent.preventDefault();
            }
        }
        function validateName(event) {
            const key = event.key;
            if (/[^a-zA-Z ]/.test(key)) {
                return false;
            }
            // Allow alphabetic characters (a-z, A-Z), spaces, and certain punctuation marks (hyphen, apostrophe)
            if (
                (key >= 'a' && key <= 'z') ||              // lowercase letters
                (key >= 'A' && key <= 'Z') ||              // uppercase letters
                key === ' ' ||                             // space
                //key === '-' ||                             // hyphen
                //key === '\'' ||                            // apostrophe
                event.keyCode === 8 ||                     // allow backspace (keyCode 8)
                event.keyCode === 9 ||                     // allow tab (keyCode 9)
                event.keyCode === 37 ||                    // allow left arrow (keyCode 37)
                event.keyCode === 39                       // allow right arrow (keyCode 39)
            ) {
                return true; // Allow the key to be entered
            } else {
                return false; // Block the key
            }
        }

        function validateOrganization(event) {
            const key = event.key;
            if (/[^a-zA-Z.0-9 ]/.test(key)) {
                return false;
            }

            // Allow alphabetic characters (a-z, A-Z), digits (0-9), spaces, and certain punctuation marks (hyphen, comma, period, apostrophe)
            if (
                (key >= 'a' && key <= 'z') ||              // lowercase letters
                (key >= 'A' && key <= 'Z') ||              // uppercase letters
                (key >= '0' && key <= '9') ||              // digits
                key === ' ' ||                             // space
                key === '-' ||                             // hyphen
                key === ',' ||                             // comma
                key === '.' ||                             // period
                //key === '\'' ||                            // apostrophe
                event.keyCode === 8 ||                     // allow backspace (keyCode 8)
                event.keyCode === 9 ||                     // allow tab (keyCode 9)
                event.keyCode === 37 ||                    // allow left arrow (keyCode 37)
                event.keyCode === 39                       // allow right arrow (keyCode 39)
            ) {
                return true; // Allow the key to be entered
            } else {
                return false; // Block the key
            }
        }

        function isNumber(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }

        function preventCountryCodeRemoval(event) {
            //alert(event);
            var phoneNumberInput = $("#phone").val();
            //var caretPosition = phoneNumberInput.selectionStart;
            var countryCode = $("#countryCode").val();
            var countryCodeLength = countryCode.length; // Adjust this value based on your country code length
            if (event.keyCode === 8 && phoneNumberInput.length <= countryCodeLength) {
                event.preventDefault();
            }
        }

        function validateEmail(event) {
            const key = event.key;
            if (key == "%" || key == "#" || key == "~" || key == "+" || key == "(" || key == ")" || key == "!" || key == "*" || key == "^" || key == "$" || key == "&" || key == "{" || key == "}" || key == ":" || key == ";" || key == '`' || key == "'" || key == '"' || key == '|' || key == '/' || key == "]" || key == "[" || key == "?" || key == "," || key == "<" || key == ">") {
                return false;
            }
        }
        function validatePhone(event) {
            const key = event.key;
            if (/[^0-9 ]/.test(key)) {
                return false;
            }
        }

        function getLengthAfterDot(email) {
            const parts = email.split('.');
            // Get the last part (substring after the last dot)
            const lastPart = parts[parts.length - 1];
            return lastPart.length;
        }

        function validateInputMail(input) {
            const emailInput = $("#email").val();
            const lengthAfterDot = getLengthAfterDot(emailInput);
            if (emailInput.includes(".") == false) {
                input.setCustomValidity('Invalid.');
            } else if (lengthAfterDot < 2) {
                input.setCustomValidity('Invalid.');
            }
            else {
                input.setCustomValidity('');

            }
        }

